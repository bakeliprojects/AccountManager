<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nogsdiva.accountmanager">

    <application
        android:name=".app.Myapp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".activity.MainActivity"/>

        <activity android:name=".activity.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activity.OperationActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity android:name=".activity.VersementActivity" />
        <activity android:name=".activity.DisplayActivity" />
        <activity android:name=".activity.RetraitActivity" />
        <activity android:name=".activity.VirementActivity" />

    </application>

</manifest>